<Window x:Class="Entering.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Entering"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="Task1">
                <TabItem.Content>
                    <Canvas>
                        <TextBlock Canvas.Left="86">Input complex parameter, left part - real, right - imaginary</TextBlock>
                        <TextBox Text="{Binding Real}" Width="100" Canvas.Left="266" Canvas.Top="33" />
                        <TextBox Text="{Binding Imaginary}" Width="100" Canvas.Left="106" Canvas.Top="33" />
                        <Button Content="Start" Height="109" Canvas.Left="106" Canvas.Top="56" Width="260" />
                    </Canvas>
                </TabItem.Content>

            </TabItem>
            <TabItem Header="Task3" />
        </TabControl>
    </Grid>
</Window>